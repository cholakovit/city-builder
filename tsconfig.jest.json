{
    "compilerOptions": {
      "jsx": "react-jsx",
      "module": "CommonJS",
      "target": "ESNext",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "strict": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["app/*"],
        "@/app/*": ["app/*"],
        "@/components/*": ["app/components/*"],
        "@/helper/*": ["app/helper/*"]
      }
    }
  }
  